{% extends '::base.html.twig' %}

{% block body -%}
    <h1>comuniAttivati</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nome</th>
                <td>{{ entity.nome }}</td>
            </tr>
            <tr>
                <th>Pathfrontimage</th>
                <td>{{ entity.pathFrontImage }}</td>
            </tr>
            <tr>
                <th>Pathfrontimageblurred</th>
                <td>{{ entity.pathFrontImageBlurred }}</td>
            </tr>
            <tr>
                <th>Datains</th>
                <td>{{ entity.dataIns|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('comuni') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('comuni_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
